        <p>
          <b>Description:</b>
          <%= @admin_store.description %>
        </p>
      </div>
      <div class="message">
        <p>
          <b>Info1:</b>
          <%= @admin_store.info1 %>
        </p>
      </div>
      <div class="message">
        <p>
          <b>Status:</b>
          <%= @admin_store.status %>
        </p>
      </div>

      <div class="message">
        <p>
          <%= link_to 'Edit', edit_admin_store_path(@admin_store) %> |
          <%= link_to 'Back', admin_stores_path %>
        </p>
      </div>
    </div>
  </div>

</article><!-- end of stats article -->
