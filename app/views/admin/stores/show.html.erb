      <div class="message">
        <p id="notice"><%= notice %></p>
      </div>
      <div class="message">
        <p>
          <b>Street1:</b>
          <%= @admin_store.street1 %>
        </p>
      </div>
      <div class="message">
        <p>
          <b>Street2:</b>
          <%= @admin_store.street2 %>
        </p>
      </div>
      <div class="message">
        <p>
          <b>City:</b>
          <%= @admin_store.city %>
        </p>
      </div>
      <div class="message">
        <p>
          <b>Postal code:</b>
          <%= @admin_store.postal_code %>
        </p>
      </div>
      <div class="message">
        <p>
          <b>Phone1:</b>
          <%= @admin_store.phone1 %>
        </p>
      </div>
      <div class="message">
        <p>
          <b>Phone2:</b>
          <%= @admin_store.phone2 %>
        </p>
      </div>
      <div class="message">
        <p>
          <b>Logo img:</b>
          <%= @admin_store.logo_img %>
        </p>
      </div>
